{
    "version": "0.1.2",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\""
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "bracketSpacing": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 120,
        "tabWidth": 4,
        "semi": true,
        "jsxSingleQuote": true,
        "endOfLine": "auto"
    },
    "name": "react-inekz",
    "author": "Nazareno Ciancaglini",
    "module": "dist/react-inekz.esm.js",
    "size-limit": [
        {
            "path": "dist/react-inekz.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/react-inekz.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.1.2",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "axios": "^1.3.2",
        "husky": "^8.0.3",
        "prettier": "^2.8.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rxjs": "^7.8.0",
        "size-limit": "^8.1.2",
        "tsdx": "^0.14.1",
        "tslib": "^2.5.0",
        "typescript": "^3.9.10"
    }
}
